<?xml version="1.0" encoding="UTF-8"?>


<hibernate-mapping package="com.latewind.bean">

    <class name="City" table="city">
        <id name="id" column="id" ><generator class="native"/> </id>
        <property name="name" type="string" column="name" /> 
        <property name="countryCode" type="string" column="CountryCode" />    
        <property name="district" type="string" column="District" />  
     <property name="population" type="integer" column="Population" />  
    </class>

</hibernate-mapping>

